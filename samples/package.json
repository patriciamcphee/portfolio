{
  "name": "smart-search-plugin-demo",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "build-tokens": "node scripts/build-tokens.js",
    "docusaurus": "npm run build-tokens && docusaurus",
    "start": "npm run build-tokens && docusaurus start --port 3001",
    "debug-build": "set NODE_OPTIONS=--inspect && set NODE_ENV=development && docusaurus build --no-minify",
    "debug-build-unix": "NODE_OPTIONS='--inspect' NODE_ENV=development docusaurus build --no-minify",
    "build": "npm run build-tokens && docusaurus build",
    "analyze": "set NODE_ENV=production && set BUNDLE_ANALYZER=true && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear && rm -rf node_modules package-lock.json",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/mdx-loader": "^3.8.1",
    "@docusaurus/plugin-client-redirects": "^3.8.1",
    "@docusaurus/plugin-content-blog": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-live-codeblock": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@emailjs/browser": "^3.11.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mdx-js/react": "^3.1.1",
    "@mermaid-js/layout-elk": "^0.1.9",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.16.4",
    "antd": "^5.27.3",
    "body-parser": "^2.2.0",
    "clsx": "^2.0.0",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "lightbox-image-plugin": "^1.0.1",
    "lucide-react": "^0.542.0",
    "markdown-to-jsx": "^7.7.13",
    "mermaid": "^11.11.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1",
    "redirects-plugin": "file:./packages/redirects-plugin",
    "redocusaurus": "^2.5.0",
    "remark-mdx": "^3.1.1",
    "remark-parse": "^10.0.2",
    "semver": "^7.5.2",
    "serve-handler": "^6.1.6",
    "smart-search-plugin": "^3.1.1",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "webpack": "^5.101.3",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/node": "^24.7.2",
    "baseline-browser-mapping": "^2.9.11",
    "shx": "^0.4.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.18.1"
  }
}
